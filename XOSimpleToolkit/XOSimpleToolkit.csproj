<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>XOSimpleToolkit</Title>
    <PackageTags>Крестики-нолики;tic-tac-toe;AI;компьютер;оппонент</PackageTags>
    <PackageId>$(AssemblyName)_SampleTest</PackageId>
	<Version>1.0.0</Version>
    <Description>Набор стратегий компьютерного оппонента и общих классов для игры в крестики-нолики</Description>
  </PropertyGroup>

</Project>
